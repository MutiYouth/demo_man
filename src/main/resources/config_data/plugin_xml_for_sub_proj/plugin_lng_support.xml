<!-- included by /META-INF/plugin.xml -->
<idea-plugin>
<extensions defaultExtensionNs="com.intellij">
    <fileType name="Simple File" implementationClass="com.weng.demo_man.language.SimpleFileType" fieldName="INSTANCE" language="Simple" extensions="simple"/>
    <lang.parserDefinition language="Simple" implementationClass="com.weng.demo_man.language.SimpleParserDefinition"/>
    <lang.syntaxHighlighterFactory language="Simple" implementationClass="com.weng.demo_man.language.SimpleSyntaxHighlighterFactory"/>
    <iconProvider implementation="com.weng.demo_man.language.SimplePropertyIconProvider"/>
    <colorSettingsPage implementation="com.weng.demo_man.language.SimpleColorSettingsPage"/>
    <annotator language="JAVA" implementationClass="com.weng.demo_man.language.SimpleAnnotator"/>
    <codeInsight.lineMarkerProvider language="JAVA" implementationClass="com.weng.demo_man.language.SimpleLineMarkerProvider"/>
    <completion.contributor language="Simple" implementationClass="com.weng.demo_man.language.SimpleCompletionContributor"/>
    <psi.referenceContributor language="JAVA" implementation="com.weng.demo_man.language.SimpleReferenceContributor"/>
    <lang.refactoringSupport language="Simple" implementationClass="com.weng.demo_man.language.SimpleRefactoringSupportProvider"/>
    <lang.findUsagesProvider language="Simple" implementationClass="com.weng.demo_man.language.SimpleFindUsagesProvider"/>
    <lang.foldingBuilder language="JAVA" implementationClass="com.weng.demo_man.language.SimpleFoldingBuilder"/>
    <gotoSymbolContributor implementation="com.weng.demo_man.language.SimpleChooseByNameContributor"/>
    <lang.psiStructureViewFactory language="Simple" implementationClass="com.weng.demo_man.language.SimpleStructureViewFactory"/>
    <navbar implementation="com.weng.demo_man.language.SimpleStructureAwareNavbar"/>
    <lang.formatter language="Simple" implementationClass="com.weng.demo_man.language.SimpleFormattingModelBuilder"/>
    <codeStyleSettingsProvider implementation="com.weng.demo_man.language.SimpleCodeStyleSettingsProvider"/>
    <langCodeStyleSettingsProvider implementation="com.weng.demo_man.language.SimpleLanguageCodeStyleSettingsProvider"/>
    <lang.commenter language="Simple" implementationClass="com.weng.demo_man.language.SimpleCommenter"/>
    <lang.documentationProvider language="Simple" implementationClass="com.weng.demo_man.language.SimpleDocumentationProvider"/>
    <spellchecker.support language="Simple" implementationClass="com.weng.demo_man.language.SimpleSpellcheckingStrategy"/>
</extensions>
</idea-plugin>